<!DOCTYPE html>
<html>
  <head>
    <%= render 'layouts/head' %>
  </head>
 
  <body>
    <header>
      <%= render 'layouts/header' %>
    </header>
      
    <div class="container jumbotron">
      <div class="container">
        <div class="row justify-content-center">
          <div class="col-6">
            <%= yield %>
          </div>
        </div>
      </div>
    </div>
    <div class="container">
      <%= debug(params) if Rails.env.development? %>
    </div>
    <footer>
      <%= render 'layouts/footer' %>
    </footer>
  </body>

  </html>